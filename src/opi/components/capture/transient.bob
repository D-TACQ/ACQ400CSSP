        <rule name="disable" prop_id="enabled" out_exp="true">
          <exp bool_exp="pv0==1">
            <expression>False</expression>
          </exp>
          <pv_name>${UUT}:MODE:CONTINUOUS</pv_name>
        </rule>
      </rules>
