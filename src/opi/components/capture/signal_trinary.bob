<display version="2.0.0">
  <name>signal trinary</name>
  <width>350</width>
  <height>30</height>
  <widget type="group" version="3.0.0">
    <name>Sig Group</name>
    <macros>
      <sig>$(sig=TRG)</sig>
    </macros>
    <width>350</width>
    <height>30</height>
    <style>3</style>
    <transparent>true</transparent>
    <widget type="label" version="2.0.0">
      <name>Sig label</name>
      <text>$(sig)</text>
      <width>70</width>
      <height>30</height>
      <transparent>false</transparent>
      <horizontal_alignment>1</horizontal_alignment>
      <vertical_alignment>1</vertical_alignment>
    </widget>
    <widget type="combo" version="2.0.0">
      <name>Sig En</name>
      <pv_name>${UUT}:$(site):${sig}</pv_name>
      <x>80</x>
      <width>90</width>
      <border_alarm_sensitive>$(SHOW_ALARM=1)</border_alarm_sensitive>
    </widget>
    <widget type="combo" version="2.0.0">
      <name>Sig Src</name>
      <pv_name>${UUT}:$(site):${sig}:DX</pv_name>
      <x>180</x>
      <width>70</width>
      <rules>
        <rule name="DISABLE" prop_id="enabled" out_exp="false">
          <exp bool_exp="pv0==0">
            <value>false</value>
          </exp>
          <exp bool_exp="true">
            <value>true</value>
          </exp>
          <pv_name>${UUT}:$(site):${sig}</pv_name>
        </rule>
      </rules>
      <border_alarm_sensitive>$(SHOW_ALARM=1)</border_alarm_sensitive>
    </widget>
    <widget type="combo" version="2.0.0">
      <name>Sig Sens</name>
      <pv_name>${UUT}:$(site):${sig}:SENSE</pv_name>
      <x>260</x>
      <width>90</width>
      <rules>
        <rule name="DISABLE" prop_id="enabled" out_exp="false">
          <exp bool_exp="pv0==0">
            <value>false</value>
          </exp>
          <exp bool_exp="true">
            <value>true</value>
          </exp>
          <pv_name>${UUT}:$(site):${sig}</pv_name>
        </rule>
      </rules>
      <border_alarm_sensitive>$(SHOW_ALARM=1)</border_alarm_sensitive>
    </widget>
  </widget>
</display>
