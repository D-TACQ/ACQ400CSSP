<display version="2.0.0">
  <name>stats values</name>
  <width>320</width>
  <height>30</height>
  <widget type="group" version="3.0.0">
    <name>Stats Group</name>
    <macros>
      <label>$(label=nolabel)</label>
      <target>$(target=GETS)</target>
    </macros>
    <width>320</width>
    <height>30</height>
    <style>3</style>
    <transparent>true</transparent>
    <widget type="label" version="2.0.0">
      <name>Stat Label</name>
      <text>$(label)</text>
      <height>30</height>
      <font>
        <font name="Default Bold" family="Liberation Sans" style="BOLD" size="14.0">
        </font>
      </font>
      <transparent>false</transparent>
      <horizontal_alignment>2</horizontal_alignment>
      <vertical_alignment>1</vertical_alignment>
    </widget>
    <widget type="textupdate" version="2.0.0">
      <name>Stat Count</name>
      <pv_name>${UUT}:0:STAT:$(target):COUNT</pv_name>
      <x>110</x>
      <height>30</height>
      <horizontal_alignment>1</horizontal_alignment>
      <vertical_alignment>1</vertical_alignment>
      <border_alarm_sensitive>false</border_alarm_sensitive>
    </widget>
    <widget type="textupdate" version="2.0.0">
      <name>Stat Rate</name>
      <pv_name>${UUT}:0:STAT:$(target):FREQ</pv_name>
      <x>220</x>
      <height>30</height>
      <horizontal_alignment>1</horizontal_alignment>
      <vertical_alignment>1</vertical_alignment>
      <border_alarm_sensitive>false</border_alarm_sensitive>
    </widget>
  </widget>
</display>
