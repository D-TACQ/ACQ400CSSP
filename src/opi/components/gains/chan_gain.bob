<display version="2.0.0">
  <name>chan gain</name>
  <width>90</width>
  <height>70</height>
  <widget type="group" version="3.0.0">
    <name>CHAN GAIN</name>
    <width>90</width>
    <height>70</height>
    <visible>false</visible>
    <style>3</style>
    <background_color>
      <color red="230" green="230" blue="230">
      </color>
    </background_color>
    <rules>
      <rule name="HIDE_EMPTY" prop_id="visible" out_exp="true">
        <exp bool_exp="&quot;$(chan)&quot;==&quot;ALL&quot;">
          <expression>true</expression>
        </exp>
        <exp bool_exp="pv0&gt;=int(&quot;$(chan)&quot;)">
          <expression>true</expression>
        </exp>
        <exp bool_exp="true">
          <expression>false</expression>
        </exp>
        <pv_name>$(UUT):$(SITE):NCHAN</pv_name>
      </rule>
    </rules>
    <widget type="group" version="3.0.0">
      <name>CHAN GAIN INNER</name>
      <x>10</x>
      <y>10</y>
      <width>70</width>
      <height>50</height>
      <style>3</style>
      <transparent>true</transparent>
    </widget>
    <widget type="label" version="2.0.0">
      <name>Label</name>
      <text>CH:$(chan)</text>
      <x>10</x>
      <y>10</y>
      <width>70</width>
      <font>
        <font name="Default Bold" family="Liberation Sans" style="BOLD" size="14.0">
        </font>
      </font>
      <horizontal_alignment>1</horizontal_alignment>
      <vertical_alignment>1</vertical_alignment>
    </widget>
    <widget type="combo" version="2.0.0">
      <name>GAIN INPUT</name>
      <pv_name>${UUT}:${SITE}:GAIN:${chan}</pv_name>
      <x>10</x>
      <y>40</y>
      <width>70</width>
      <height>20</height>
      <background_color>
        <color name="Background" red="255" green="255" blue="255">
        </color>
      </background_color>
      <border_alarm_sensitive>false</border_alarm_sensitive>
    </widget>
  </widget>
</display>
