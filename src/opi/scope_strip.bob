<display version="2.0.0">
  <name>$(SCOPE_TITLE)</name>
  <macros>
    <WIDTH>$(WIDTH=800)</WIDTH>
    <HEIGHT>$(HEIGHT=600)</HEIGHT>
    <TRACE_WIDTH>$(TRACE_WIDTH=1)</TRACE_WIDTH>
    <SCOPE_TITLE>$(SCOPE_TITLE=LIVE SCOPE)</SCOPE_TITLE>
  </macros>
